entity DataType{
    name String
}

entity InteractionType{
    name String
}

entity Scenario{
    name String
}

entity Session{
    sourceFile Blob
}

entity DataRecord{
    duration Integer
    text String
    sourceId String
    time Integer
}

relationship ManyToOne{
    DataRecord to Session,
    DataRecord to InteractionType
}

relationship ManyToOne{
    Session to DataType
}

relationship ManyToOne{
    Session{scenario(name)} to Scenario{session}
}
